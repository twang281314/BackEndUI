{
  "name": "blog",
  "description": "Mo's Blog",
  "version": "0.4.0",
  "author": "Mu Haibao<mhbseal@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhbseal/blog"
  },
  "scripts": {
    "start.api": "node ./bin/apiServer",
    "start.api.noauth": "node ./bin/apiServer noauth",
    "start.render": "node ./bin/renderServer",
    "start.webpack": "node ./fe/build/webpack.dev.server",
    "dev": "npm run start.api & npm run start.render & npm run start.webpack",
    "dev.noauth": "npm run start.api.noauth & npm run start.render & npm run start.webpack",
    "pm2.start.api": "NODE_ENV=production pm2 start ./bin/apiServer.js --name blog.api --watch",
    "pm2.start.render": "NODE_ENV=production pm2 start ./bin/renderServer.js --name blog.render --watch",
    "pm2.reload.api": "NODE_ENV=production pm2 reload ./bin/apiServer.js --name blog.api --watch",
    "pm2.reload.render": "NODE_ENV=production pm2 reload ./bin/renderServer.js --name blog.render --watch",
    "build": "webpack --verbose --colors --display-error-details --config ./fe/build/webpack.config.prod",
    "nginx.start": "/usr/local/nginx/sbin/nginx",
    "nginx.reload": "/usr/local/nginx/sbin/nginx -s reload",
    "chmod": "chmod -R +x bin",
    "mongod.dev": "bin/mongod.dev.sh",
    "mongodump.dev": "bin/mongodump.dev.sh",
    "mongorestore.dev": "bin/mongorestore.dev.sh",
    "mongod.prod": "bin/mongod.prod.sh",
    "mongodump.prod": "bin/mongodump.prod.sh",
    "mongorestore.prod": "bin/mongorestore.prod.sh",
    "tar.dev": "bin/tar.dev.sh",
    "tar.prod": "bin/tar.prod.sh",
    "scp.dev": "bin/scp.dev.sh",
    "data.dev": "npm run scp.dev && npm run tar.dev && npm run mongorestore.dev",
    "data.prod": "npm run mongodump.prod && npm run tar.prod",
    "lint": "eslint ./fe ./be"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.0",
    "co": "^4.6.0",
    "compression": "^1.6.0",
    "cookie-session": "^1.2.0",
    "express": "^4.13.3",
    "history": "^1.13.0",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "mo2js": "^0.2.1",
    "mongoose": "^4.2.7",
    "morgan": "^1.6.1",
    "multer": "^1.1.0",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.4.0",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.3",
    "babel-plugin-react-transform": "^1.1.1",
    "clean-webpack-plugin": "^0.1.4",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.13.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "json-loader": "^0.5.3",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-router": "^1.0.0-beta4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": ">=4"
  }
}
